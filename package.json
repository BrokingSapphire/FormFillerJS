{
  "name": "formfillerjs",
  "version": "1.0.0",
  "description": "A lightweight Node.js tool for dynamically filling PDF forms asynchronously using pdf-lib. Supports user input, CLI-based interaction, and environment configuration.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "tsc && node dist/server.js",
    "watch-and-run": "nodemon --watch dist/ dist/server.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "pdf",
    "pdf-lib",
    "form-filling",
    "dynamic-pdf",
    "nodejs",
    "async",
    "dotenv"
  ],
  "author": "Nakul Thakur",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
